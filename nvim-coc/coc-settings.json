{
  "rust-analyzer.enable": true,
  "rust-analyzer.server.path": "/usr/local/bin/rust-analyzer",
  "eslint.autoFixOnSave": true,
  "suggest.completionItemKindLabels": {
    "variable": "\uf71b",
    "constant": "\uf8ff",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\ufa52",
    "text": "\ue612",
    "enum": "\uf0ca",
    "enumMember": "\uf02b",
    "color": "\ue22b",
    "property": "\ufab6",
    "field": "\uf635",
    "function": "\uf6a6",
    "method": "\uf6a6",
    "unit": "\uf475",
    "file": "\uf471",
    "value": "\uf8ae",
    "event": "\ufacd",
    "folder": "\uf115",
    "keyword": "\uf805",
    "module": "\uf487",
    "snippet": "\uf64d",
    "operator": "\uf915",
    "reference": "\uf87a",
    "typeParameter": "\uf278",
    "default": "\uf29c"
  },
  "suggest.disableKind": false,
  "suggest.disableMenu": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "filetypes": ["go"],
      "initializationOptions": {
        "gocodeCompletionEnable": true,
        "diagnosticsEnabled": true,
        "lintTool": "golint"
      }
    }
  }
}
